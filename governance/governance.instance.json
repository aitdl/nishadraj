        "direct_commit_blocked": true,
        "pull_request_required": true
    },
    "trademark_protection": {
        "active": true,
        "brand_name_protected": "NishadRaj OS",
        "unauthorized_use_blocked": true
    },
    "hybrid_expansion_layer": {
        "global_outreach_enabled": true,
        "academic_partnerships_enabled": true,
        "institutional_engagement_enabled": true,
        "sustainable_revenue_model_enabled": true,
        "neutrality_enforced": true
    },
    "block_if_schema_mismatch": true,
    "block_execution_if_missing": true
